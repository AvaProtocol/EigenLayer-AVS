{
  "name": "ava-sdk-timeout-tests",
  "version": "1.0.0",
  "description": "Test suite for timeout functionality in the Ava Protocol SDK",
  "main": "example.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.11.1",
    "@grpc/proto-loader": "^0.7.13"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["**/*.test.ts", "**/*.test.js"],
    "collectCoverageFrom": [
      "**/*.{ts,js}",
      "!**/*.test.{ts,js}",
      "!**/node_modules/**"
    ]
  }
}