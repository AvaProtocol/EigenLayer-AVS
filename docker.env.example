# EigenLayer AVS Docker Environment Variables
# Copy this file to .env and fill in your actual values

# Core RPC Configuration (REQUIRED)
ETH_RPC_URL=your_ethereum_rpc_url_here
ETH_WS_URL=your_ethereum_websocket_url_here

# Private Keys (REQUIRED - no defaults for security)
ECDSA_PRIVATE_KEY=your_aggregator_private_key_here
CONTROLLER_PRIVATE_KEY=your_smart_wallet_controller_private_key_here
OPERATOR_PRIVATE_KEY=your_operator_private_key_here

# JWT Secret for authentication
JWT_SECRET=your_jwt_secret_here

# API Keys and Tokens
TENDERLY_ACCESS_KEY=your_tenderly_access_key_here
AP_NOTIFY_BOT_TOKEN=your_telegram_bot_token_here
SENDGRID_KEY=your_sendgrid_api_key_here
THEGRAPH_API_KEY=your_thegraph_api_key_here

# Smart Wallet Configuration
BUNDLER_URL=your_bundler_rpc_url_here
MAX_WALLETS_PER_OWNER=5

# Optional Overrides
TENDERLY_ACCOUNT=your_tenderly_account
TENDERLY_PROJECT=your_tenderly_project
SERVER_NAME=aggregator-local
OPERATOR_SERVER_NAME=operator-local
AGGREGATOR_ADDRESS=aggregator:2206

# Test Environment Variables (for docker-compose-test.yml)
BASE_SEPOLIA_RPC_URL=your_base_sepolia_rpc_url_here
BASE_SEPOLIA_WS_URL=your_base_sepolia_websocket_url_here
BUNDLER_PRIVATE_KEY=your_test_bundler_private_key

# Test Keys (can be different from production)
TEST_ECDSA_PRIVATE_KEY=your_test_aggregator_private_key
TEST_CONTROLLER_PRIVATE_KEY=your_test_controller_private_key
TEST_OPERATOR_PRIVATE_KEY=your_test_operator_private_key
TEST_JWT_SECRET=test_jwt_secret

# Test API Keys
TEST_TENDERLY_ACCESS_KEY=your_test_tenderly_access_key
TEST_AP_NOTIFY_BOT_TOKEN=your_test_telegram_bot_token
TEST_SENDGRID_KEY=your_test_sendgrid_key
TEST_THEGRAPH_API_KEY=your_test_thegraph_api_key
TEST_MAX_WALLETS_PER_OWNER=5
TEST_TENDERLY_ACCOUNT=your_test_tenderly_account
TEST_TENDERLY_PROJECT=your_test_tenderly_project
