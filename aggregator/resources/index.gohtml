<html lang="en">
<head>
    <meta charset="UTF-8"/>
    <title>Ava Protocol - Operator Dashboard</title>
    <meta name="description" content="Ava Protocol operator dashboard showing real-time status of EigenLayer AVS operators">
    <meta name="keywords" content="Ava Protocol, EigenLayer, AVS, operators, blockchain, ethereum">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" type="image/x-icon" href="/favicon.ico">
    <script src="https://cdn.tailwindcss.com"></script>
    
    <!-- Open Graph / Facebook -->
    <meta property="og:type" content="website">
    <meta property="og:url" content="https://app.avaprotocol.org/">
    <meta property="og:title" content="Ava Protocol - Operator Dashboard">
    <meta property="og:description" content="Ava Protocol operator dashboard showing real-time status of EigenLayer AVS operators">
    
    <!-- Twitter -->
    <meta property="twitter:card" content="summary_large_image">
    <meta property="twitter:url" content="https://app.avaprotocol.org/">
    <meta property="twitter:title" content="Ava Protocol - Operator Dashboard">
    <meta property="twitter:description" content="Ava Protocol operator dashboard showing real-time status of EigenLayer AVS operators">
</head>
<body class="bg-black">
<div class="bg-black">
  <div class="mx-auto max-w-7xl py-24 sm:px-6 sm:py-32 lg:px-8">
    <ul role="list" class="divide-y divide-gray-800">
      {{ range .Nodes }}
      <li class="flex justify-between gap-x-6 py-5">
        <div class="flex min-w-0 gap-x-4">
          {{/* TODO: Parse metadata to get image url <img class="h-12 w-12 flex-none rounded-full bg-gray-50"
    src="https://testnet-usea1-holesky-operator-metadata.s3.amazonaws.com/0x3c7a1a9c769009d21fe2fcfb08c7334919f641fe/logo.png"
    alt="">*/}}
          <div class="min-w-0 flex-auto">
            <p class="text-sm font-semibold leading-6 text-white">
              <a href="{{ .EtherscanURL }}">
                {{ if ne .Name "" }}{{ .Name }}{{ else }}{{ .Address }}{{ end }}
              </a>
            </p>
            <p class="mt-1 truncate text-xs leading-5 text-gray-400">
              {{ if ne .Name "" }}{{ .Address }}{{ end }}
            </p>
            <p class="mt-1 truncate text-xs leading-5 text-gray-400">
              <a href="{{ .EigenlayerURL }}">View on EigenLayer</a>
            </p>
          </div>
        </div>

        <div class="flex min-w-0 gap-x-4">
          <div class="min-w-0 flex-auto">
            {{ if ne .Version "" }}
            <p class="text-sm font-semibold leading-6 text-white">
              {{ .Version }}
            </p>
            {{ end }}
            <!--
            {{ if gt .MetricsPort 0 }}
            <p class="text-sm leading-6 text-white">
              <a href="http://{{ .RemoteIP }}:{{ .MetricsPort }}/metrics">Metric</a>
            </p>
            {{ end }}-->
          </div>
        </div>


        <div class="hidden shrink-0 sm:flex sm:flex-col sm:items-end">
          <p class="text-sm leading-6 text-white">Active</p>
          <p class="mt-1 text-xs leading-5 text-gray-400">Last seen <time datetime="2023-01-23T13:23Z">{{ .LastSeen }}</time></p>
          <p class="mt-1 text-xs leading-5 text-gray-400">Block {{ .BlockNumer }}</p>
          <p class="mt-1 text-xs leading-5 text-gray-400">Event {{ .EventCount }}</p>
        </div>
        {{ end }}
      </li>
    </ul>
  </div>
    <!-- Center text for version and revision -->
    <div class="text-center mb-8">
      <p class="text-white text-sm">
        Aggregator {{.Version}}
        <a href="https://github.com/AvaProtocol/EigenLayer-AVS/commit/{{.Revision}}" class="underline" target="_blank">+{{.Revision}}</a>
      </p>
    </div>
</div>
</body>
